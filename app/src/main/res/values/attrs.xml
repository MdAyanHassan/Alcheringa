<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ToolDotProgress">
        <attr name="color" format="color" />
        <attr name="count" format="integer" />
        <attr name="timeout" format="integer" />
    </declare-styleable>

    <declare-styleable name="ShadowIndicatorBottomNavigationView">
        <attr name="indicatorHeaderHeight" format="dimension|reference"/>
        <attr name="indicatorHeaderColor" format="reference|color"/>
        <attr name="indicatorShadowColor" format="reference|color"/>
        <attr name="indicatorShadowVisible" format="boolean"/>
    </declare-styleable>
    <declare-styleable name="TextViewOutline">
        <attr name="outlineSize" format="dimension"/>
        <attr name="outlineColor" format="color|reference"/>
        <attr name="android:shadowRadius"/>
        <attr name="android:shadowDx"/>
        <attr name="android:shadowDy"/>
        <attr name="android:shadowColor"/>
    </declare-styleable>
</resources>

