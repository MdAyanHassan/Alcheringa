<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="ToolDotProgress">
        <attr name="color" format="color" />
        <attr name="count" format="integer" />
        <attr name="timeout" format="integer" />
    </declare-styleable>

    <declare-styleable name="ShadowIndicatorBottomNavigationView">
        <attr name="indicatorHeaderHeight" format="dimension|reference"/>
        <attr name="indicatorHeaderColor" format="reference|color"/>
        <attr name="indicatorShadowColor" format="reference|color"/>
        <attr name="indicatorShadowVisible" format="boolean"/>
    </declare-styleable>
</resources>

